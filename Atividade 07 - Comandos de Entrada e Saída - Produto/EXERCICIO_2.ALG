Algoritmo "Exercicio_2"

Var

   Salario:real

Inicio

      escreva("INFORME O SEU SALÁRIO ATUAL: ")
      leia(Salario)
      
      limpatela
      escreval("SALÁRIO ATUAL: R$",Salario)
      escreval("PERCENTUAL DE REAJUSTE: 25%")
      escreval("VALOR DE REAJUSTE: R$",Salario*0.25)
      escreval("SALÁRIO REAJUSTADO: R$",Salario*1.25)

Fimalgoritmo