Algoritmo "Exercicio_4"

Var

   num, num2:inteiro

Inicio

      escreva("INSIRA O PRIMEIRO NÚMERO: ")
      leia(num)
      escreva("INSIRA O SEGUNDO NÚMERO: ")
      leia(num2)
      escreval()
      
      se(num>num2)entao
      escreva("O NÚMERO ",num," É MAIOR")
      senao
      escreva("O NÚMERO ",num2," É MAIOR")
      fimse


Fimalgoritmo