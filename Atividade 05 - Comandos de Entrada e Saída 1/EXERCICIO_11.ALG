   idade1,idade2,idade3,idade4,idade5:inteiro
   tel1,tel2,tel3,tel4,tel5:inteiro

Inicio
      //Coleta de dados
      escreva("Informe o nome da primeira pessoa: ")
      leia(nome1)
      escreva("Informe a idade da primeira pessoa: ")
      leia(idade1)
      escreva("Informe o telefone da primeira pessoa: ")
      leia(tel1)
      escreval()
