      leia(idade4)
      escreva("Informe o telefone da quarta pessoa: ")
      leia(tel4)
      escreval()
      escreva("Informe o nome da quinta pessoa: ")
      leia(nome5)
      escreva("Informe a idade da quinta pessoa: ")
      leia(idade5)
      escreva("Informe o telefone da quinta pessoa: ")
      leia(tel5)

      
      
      //Execução do programa
      limpatela
      escreval("DADOS PRIMEIRA PESSOA")
      escreval("NOME: ",nome1)
      escreval("IDADE: ",idade1)
      escreval("TELEFONE: (41)",tel1)
      escreval()
      escreval("DADOS SEGUNDA PESSOA")
      escreval("NOME: ",nome2)
      escreval("IDADE: ",idade2)
      escreval("TELEFONE: (41)",tel2)
      escreval()
      escreval("DADOS TERCEIRA PESSOA")
      escreval("NOME: ",nome3)
      escreval("IDADE: ",idade3)
      escreval("TELEFONE: (41)",tel3)
      escreval()
      escreval("DADOS QUARTA PESSOA")
      escreval("NOME: ",nome4)
      escreval("IDADE: ",idade4)
      escreval("TELEFONE: (41)",tel4)
      escreval()
      escreval("DADOS QUINTA PESSOA")
      escreval("NOME: ",nome5)
      escreval("IDADE: ",idade5)
      escreval("TELEFONE: (41)",tel5)
      escreval()
      


Fimalgoritmo