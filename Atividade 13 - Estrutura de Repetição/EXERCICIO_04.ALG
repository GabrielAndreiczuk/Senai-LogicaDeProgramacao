algoritmo "Exercicio_4"

var

   i:inteiro

inicio

   escreval("NÚMEROS PARES DE 20 À 40 NA HORIZONTAL")
   escreval()
   i:=20
   enquanto (i<=40)faca
      se(i mod 2 = 0)entao
         escreva(i,", ")
         i:=i+1
      senao
         i:=i+1
      fimse
   fimenquanto

fimalgoritmo