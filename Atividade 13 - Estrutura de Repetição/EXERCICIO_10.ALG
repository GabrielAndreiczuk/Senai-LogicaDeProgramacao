algoritmo "Exercicio_10"

var

   i:inteiro

inicio

   escreval("CONTAGEM REGRASSIVA NÚMEROS PARES DE 50 ATÉ 0")
   escreval()
   i:=50
   enquanto (i>=0)faca
      se(i mod 2 = 0)entao
         escreva(i,", ")
         i:=i-1
      senao
         i:=i-1
      fimse
   fimenquanto

fimalgoritmo