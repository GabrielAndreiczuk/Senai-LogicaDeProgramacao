algoritmo "Exercicio_8"

var

   i:inteiro

inicio

   escreval("NÚMEROS MÚLTIPLOS DE 5, ENTRE 0 ATÉ 100")
   escreval()
   i:=0
   enquanto (i<=100)faca
      se(i mod 5 = 0)entao
         escreva(i,", ")
         i:=i+1
      senao
         i:=i+1
      fimse
   fimenquanto

fimalgoritmo