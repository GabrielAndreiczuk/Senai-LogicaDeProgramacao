algoritmo "Exercicio_5"

var

   i:inteiro

inicio

   escreval("NÚMEROS PARES DE 10 À 60 NA VERTICAL")
   escreval()
   i:=10
   enquanto (i<=60)faca
      se(i mod 2 <> 0)entao
         escreval(i,", ")
         i:=i+1
      senao
         i:=i+1
      fimse
   fimenquanto

fimalgoritmo