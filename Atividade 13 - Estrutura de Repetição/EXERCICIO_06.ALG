algoritmo "Exercicio_6"

var

   i:inteiro

inicio

   escreval("NÚMEROS MÚLTIPLOS DE 3, ENTRE 40 ATÉ 100 NA HORIZONTAL")
   escreval()
   i:=40
   enquanto (i<=100)faca
      se(i mod 3 = 0)entao
         escreva(i,", ")
         i:=i+1
      senao
         i:=i+1
      fimse
   fimenquanto

fimalgoritmo